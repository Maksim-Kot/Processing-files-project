<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MainGUIController">
   <GridPane>
     <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextField fx:id="unmodFolder" GridPane.columnIndex="3" GridPane.rowIndex="1" />
         <Button fx:id="unmodButton" mnemonicParsing="false" onAction="#unmodButtonClick" text="Submit" GridPane.columnIndex="4" GridPane.rowIndex="1" />
         <ChoiceBox fx:id="unmod" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <ChoiceBox fx:id="unmodFile" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         <Label text="Unmodifiy file(s)" />
         <Label text="Type of modification" GridPane.columnIndex="1" />
         <Label text="Name of file" GridPane.columnIndex="2" />
         <Label text="Folder to save" GridPane.columnIndex="3" />
         <Label text="Process file(s)" GridPane.rowIndex="3" />
         <TextField fx:id="procNameOfFolder" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         <TextField fx:id="procFolderToSave" GridPane.columnIndex="1" GridPane.rowIndex="8" />
         <TextField fx:id="procNameOfFile" GridPane.columnIndex="3" GridPane.rowIndex="8" />
         <Label text="Name of folder" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         <Label text="Type of reading" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         <Label text="Type of calculation" GridPane.columnIndex="2" GridPane.rowIndex="5" />
         <Label text="Folder to save" GridPane.columnIndex="1" GridPane.rowIndex="7" />
         <Label text="Type of writing" GridPane.columnIndex="2" GridPane.rowIndex="7" />
         <Label text="Name of file" GridPane.columnIndex="3" GridPane.rowIndex="7" />
         <Button fx:id="procButton" mnemonicParsing="false" onAction="#procButtonClick" text="Submit" GridPane.columnIndex="4" GridPane.rowIndex="8" />
         <ChoiceBox fx:id="procTypeReading" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
         <ChoiceBox fx:id="procTypeCalc" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="6" />
         <ChoiceBox fx:id="procTypeWriting" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="8" />
         <ChoiceBox fx:id="procFileToCalc" prefWidth="150.0" GridPane.columnIndex="3" GridPane.rowIndex="4" />
         <Label text="File to calculate" GridPane.columnIndex="3" GridPane.rowIndex="3" />
         <Button fx:id="folderButton" mnemonicParsing="false" onAction="#folderButtonClick" text="Submit folder" GridPane.columnIndex="2" GridPane.rowIndex="4" />
         <Button fx:id="calcButton" mnemonicParsing="false" onAction="#calcButtonClick" text="Submit calculation" GridPane.columnIndex="3" GridPane.rowIndex="6" />
      </children>
   </GridPane>
   <TextArea fx:id="outArea" prefHeight="200.0" prefWidth="200.0" />
</VBox>
